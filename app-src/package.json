{
  "name": "star2star-native-web-starter-kit",
  "version": "0.0.2",
  "description": "Star2Star Native Web Starter Kit ",
  "main": "index.web.js",
  "scripts": {
    "start": "NODE_ENV=development node webpack-dev-server --hot --inline",
    "clean": "rm -rf node_modules dist package-lock.json; npm i;",
    "test": "jest --verbose --coverage",
    "build": "NODE_ENV=production webpack -p && cd dist; zip -r ../star2star-native-web-starter-kit.zip *",
    "ui-test": "node ./node_modules/ui-harness/start  --entry='./src/tests/specs'"
  },
  "babel": {
    "plugins": [
      "react-native-web/babel"
    ],
    "presets": [
      "es2015",
      "react",
      "react-native",
      "stage-0"
    ]
  },
  "jest-babel": {
    "stage": 0
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "styles.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/tests/**/*.js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/specs/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/sinon",
      "<rootDir>/src/components",
      "<rootDir>/node_modules/s2s-base-class"
    ]
  },
  "keywords": [],
  "author": "Star2Star Communications, LLC",
  "companyName": "Star2Star Communicaions, LLC",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^13.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "enzyme": "^2.4.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^13.2.3",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.5",
    "ui-harness": "^3.17.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {
    "form-data": "^2.1.4",
    "immutable": "^4.0.0-rc.1",
    "md5": "^2.1.0",
    "moment": "^2.19.1",
    "object-merge": "^2.5.1",
    "opentok-accelerator-core": "*",
    "opentok-solutions-css": "*",
    "promise-defer": "^1.0.0",
    "react": "15.4.2",
    "react-a11y": "^0.3.3",
    "react-async-component": "^1.0.0-beta.3",
    "react-dom": "15.4.2",
    "react-intl": "^2.1.3",
    "react-modal": "^1.4.0",
    "react-motion": "^0.4.4",
    "react-native-web": "0.0.130",
    "react-redux": "^5.0.4",
    "react-router-dom": "v4.1.1",
    "react-router-transition": "^0.1.1",
    "recompose": "^0.23.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "redux-tooltip": "^0.5.4",
    "reselect": "^3.0.0",
    "s2s-action": "*",
    "s2s-alert-list": "*",
    "s2s-app-service": "*",
    "s2s-avatar": "*",
    "s2s-base-class": "*",
    "s2s-button": "*",
    "s2s-button-bar": "*",
    "s2s-checkbox": "*",
    "s2s-logging-service": "*",
    "s2s-login": "*",
    "s2s-select": "*",
    "s2s-status": "*",
    "s2s-svg-action": "*",
    "s2s-svg-content": "*",
    "s2s-svg-graphics": "*",
    "s2s-svg-navigation": "*",
    "s2s-themes": "*",
    "styled-components": "^2.2.3"
  }
}
